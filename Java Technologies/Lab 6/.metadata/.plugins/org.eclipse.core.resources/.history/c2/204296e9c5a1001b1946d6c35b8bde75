import java.util.*;
import java.lang.*;
class Calculator<T extends Number>{
	T[] numbers;
	Calculator(T[] o) {
		numbers = o;
	}
	double addition(){
		double sum = 0;
		for(T t : numbers){
			sum += t.doubleValue();
		}
		return sum;
	}
	//double multiplication
}
public class TestClass {
	public static void main(String args[]) {
		Integer nums[] = {2,3};
		Calculator<Integer> c = new Calculator<Integer>(nums);	
		System.out.println(c.addition());
	}

	
}
